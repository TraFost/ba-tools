{
	"rewrites": [
		{ "source": "/api/(.*)", "destination": "/api/$1" },
		{ "source": "/((?!api/).*)", "destination": "/404" }
	],
	"version": 2,
	"buildCommand": "prisma generate && pnpm build",
	"devCommand": "pnpm dev",
	"installCommand": "pnpm install",
	"framework": "nextjs",
	"github": {
		"silent": true,
		"autoJobCancelation": true
	}
}
